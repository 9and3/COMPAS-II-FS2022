                    <item name="CodeInput" type_name="gh_string" type_code="10">from compas.geometry import Sphere
from compas.geometry import Vector
from compas.artists import Artist

def sphere_center_generator():
    sphere = Sphere((0.35, 0, 0), 0.15)
    for x in range(5):
        for z in range(7):
            center = sphere.point + Vector(x, 0, z) * 0.05
            yield center

centers = list(sphere_center_generator())

P = [Artist(c).draw() for c in centers]

import Rhino.Geometry as rg
spheres = [rg.Sphere(c, 0.15) for c in P]</item>
