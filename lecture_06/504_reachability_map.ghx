                  <items count="13">
                    <item name="CodeInput" type_name="gh_string" type_code="10">from scriptcontext import sticky as st

import compas

from compas_fab.robots.ur5 import Robot
from compas_ghpython.artists import RobotModelArtist

# Robot meshes are usually in meters
# Set a high precision to prevent parsing errors
# and configure Rhino itself in meters
compas.PRECISION = '12f'

# Store robot in component-based key
robot_key = 'robot_{}'.format(str(ghenv.Component.InstanceGuid))

if robot_key not in st:
    st[robot_key] = None

if load:
    robot = Robot(load_geometry=True)
    robot.artist = RobotModelArtist(robot.model)

    st[robot_key] = robot

robot = st[robot_key]
