                    <item name="CodeInput" type_name="gh_string" type_code="10">from System.Drawing import Color
from compas_ghpython.utilities import draw_frame
from compas.geometry import Frame

red = Color.FromArgb(255, 0, 0)
green = Color.FromArgb(0, 255, 0)
blue = Color.FromArgb(0, 0, 255)

P = []
V = []
C = []

scale = float(scale)

for frame in frames:
    
    if not frame:
        continue
    if isinstance(frame, Frame):
        plane = draw_frame(frame)
    else:
        plane = frame
    P += [plane.Origin] * 3
    V.append(plane.XAxis * scale)
    V.append(plane.YAxis * scale)
    V.append(plane.ZAxis * scale)
    C += [red, green, blue]

W = 3</item>
