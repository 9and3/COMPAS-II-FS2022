import Rhino.Geometry as rg

def sphere_generator():
    sphere = Sphere((0.35, 0, 0), 0.15)
    for x in range(5):
        for z in range(7):
            center = sphere.point + Vector(x, 0, z) * 0.05
            yield Sphere(center, sphere.radius)

spheres = [s for s in sphere_generator()]

sphere_centers = [s.point for s in spheres]

from compas.artists import ShapeArtist

a = [rg.Sphere(rg.Point3d(*list(s.point)), s.radius) for s in spheres]

