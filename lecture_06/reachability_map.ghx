P = []
V = []
C = []

scale = float(scale)

for frame in frames:
    
    if not frame:
        continue
    if isinstance(frame, Frame):
        plane = draw_frame(frame)
    else:
        plane = frame
    P += [plane.Origin] * 3
    V.append(plane.XAxis * scale)
    V.append(plane.YAxis * scale)
    V.append(plane.ZAxis * scale)
    C += [red, green, blue]

